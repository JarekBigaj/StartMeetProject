@model IEnumerable<AppUser>

<div class="bg-primary m-1 p-1 text-white">
    <h4>
        User accounts
    </h4>
</div>
<table class="table table-sm table-bordered">
    <tr>
        <th>ID</th>
        <th>User Name</th>
        <th>E-mail Adress</th>
    </tr>
    @if(Model.Count()==0)
    {
        <tr>
            <td colspan="3" class="text-center">No User accounts.</td>
        </tr>
    }
    else
    {
        foreach (AppUser user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.UserSecondName</td>
                <td>@user.Email</td>
            </tr>    
        }
    }
</table>